<!--pages/miaowu/miaowu.wxml-->
<!-- 头部 -->
<view class="top">
    <image src="/images/喵.png" class="log"></image>
    <view class="top-right">
        <image src="/images/铃铛.png"></image>
        <image src="/images/猫人.png"></image>
    </view>
</view>
<!-- tab栏 -->
<view class="tab-box">
    <van-tabs active="{{ active }}" bind:change="onChange" title-active-color="#f15a24" color="#f15a24" line-width="80px" swipeable="true">
        <van-tab title="广场">
            <!-- 滑动模块 -->
            <scroll-view scroll-x="true">
                <view class="hua-box">
                    <view class="hua-item">
                        <view class="img">
                            <image src="https://lexuemiao.oss-cn-beijing.aliyuncs.com/uploads/image/2020af1zvps41607912321.jpg"></image>
                        </view>
                        <view class="title">···</view>
                        <view class="num">查看更多考研话题</view>
                    </view>
                    <view class="hua-item" wx:for="{{indexMore}}" wx:key="index">
                        <view class="img">
                            <image src="{{item.background}}"></image>
                        </view>
                        <view class="title">#{{item.title}}#</view>
                        <view class="num">{{item.dyna_count}}讨论</view>
                    </view>
                </view>
            </scroll-view>
            <block wx:for="{{infolist}}" wx:key="item.square_id">
                <view class="release">
                    <view class="release-top">
                        <view class="left">
                            <!-- 头像 -->
                            <view class="img">
                                <image src="{{item.square_info.avatar}}" class="to"></image>
                                <image src="/images/v.png" class="v"></image>
                            </view>
                            <view class="name-box">
                                <!-- 账号名 -->
                                <view>{{item.square_info.user_nickname}}</view>
                                <!-- 日期 -->
                                <view class="time">08-19</view>
                            </view>
                        </view>
                        <!-- 关注 -->
                        <view class="right">
                            <view class="guan">
                                +关注</view>
                        </view>
                        <!-- 三个点 -->
                        <view class="dian">
                            <text></text>
                            <text></text>
                            <text></text>
                        </view>
                    </view>
                    <!-- 内容 -->
                    <view class="substance">
                        <view class="substance-title">{{item.square_info.dyna_details}}</view>
                        <view class="substance-img">
                            <image src="{{item.square_info.dyna_cover}}"></image>
                        </view>
                    </view>
                    <!-- 尾部 -->
                    <view class="footer">
                        <view>
                            <image src="data:image/png;base64,"></image>
                        </view>
                        <view>
                            <image src="data:image/png;base64,"></image>
                            <text>9</text>
                        </view>
                        <view>
                            <image src="data:image/png;base64,"></image>
                            <text>30</text>
                        </view>
                    </view>
                </view>
            </block>

        </van-tab>
        <van-tab title="{{tabBar}}">
            <block wx:for="{{booklist}}" wx:key="item.id">
                <view class="release">
                    <view class="release-top">
                        <view class="left">
                            <!-- 头像 -->
                            <view class="img">
                                <image src="{{item.avatar}}" class="to"></image>
                                <image src="/images/v.png" class="v"></image>
                            </view>
                            <view class="name-box">
                                <!-- 账号名 -->
                                <view>{{item.user_nickname}}</view>
                                <!-- 日期 -->
                                <view class="time">08-19</view>
                            </view>
                        </view>
                        <!-- 关注 -->
                        <view class="right">
                            <view class="guan">
                                +关注</view>
                        </view>
                        <!-- 三个点 -->
                        <view class="dian">
                            <text></text>
                            <text></text>
                            <text></text>
                        </view>
                    </view>
                    <!-- 内容 -->
                    <view class="substance">
                        <view class="substance-img">
                            <image src="{{item.cover}}"></image>
                        </view>
                        <view>
                        {{item.content}}
                        </view>
                    </view>
                    <!-- 尾部 -->
                    <view class="footer">
                        <view>
                            <image src="data:image/png;base64,"></image>
                        </view>
                        <view>
                            <image src="data:image/png;base64,"></image>
                            <text>9</text>
                        </view>
                        <view>
                            <image src="data:image/png;base64,"></image>
                            <text>30</text>
                        </view>
                    </view>
                </view>
            </block>
        </van-tab>
        <van-tab title="关注">
            <block wx:for="{{infolist}}" wx:key="item.square_id">
                <view class="release">
                    <view class="release-top">
                        <view class="left">
                            <!-- 头像 -->
                            <view class="img">
                                <image src="{{item.square_info.avatar}}" class="to"></image>
                                <image src="/images/v.png" class="v"></image>
                            </view>
                            <view class="name-box">
                                <!-- 账号名 -->
                                <view>{{item.square_info.user_nickname}}</view>
                                <!-- 日期 -->
                                <view class="time">08-19</view>
                            </view>
                        </view>
                        <!-- 关注 -->
                        <view class="right">
                            <view class="guan">
                                +关注</view>
                        </view>
                        <!-- 三个点 -->
                        <view class="dian">
                            <text></text>
                            <text></text>
                            <text></text>
                        </view>
                    </view>
                    <!-- 内容 -->
                    <view class="substance">
                        <view class="substance-title">{{item.square_info.dyna_details}}</view>
                        <view class="substance-img">
                            <image src="{{item.square_info.dyna_cover}}"></image>
                        </view>
                    </view>
                    <!-- 尾部 -->
                    <view class="footer">
                        <view>
                            <image src="data:image/png;base64,"></image>
                        </view>
                        <view>
                            <image src="data:image/png;base64,"></image>
                            <text>9</text>
                        </view>
                        <view>
                            <image src="data:image/png;base64,"></image>
                            <text>30</text>
                        </view>
                    </view>
                </view>
            </block>
        </van-tab>
    </van-tabs>
</view>
<!-- 发布 -->
<!-- 定位 -->
<view class="miao"></view>